# PostgreSQL World Configuration
# Copy this file to .env and fill in your values

# PostgreSQL Connection
# Format: postgres://username:password@host:port/database
WORKFLOW_POSTGRES_URL="postgres://postgres:password@localhost:5432/workflow_dev"

# Workflow Configuration
WORKFLOW_TARGET_WORLD="@workflow/world-postgres"
WORKFLOW_POSTGRES_JOB_PREFIX="workflow_"
WORKFLOW_POSTGRES_WORKER_CONCURRENCY=10

# Optional: Claude Code Provider Configuration
# These are already configured via 'claude login', but can be overridden
# ANTHROPIC_API_KEY=your_api_key_here
